<Application x:Class="MichaelBrandonMorris.FileSystemSizeInfoTool.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:valueConverters="clr-namespace:MichaelBrandonMorris.ValueConverters;assembly=MichaelBrandonMorris.ValueConverters"
             xmlns:fileSystemSizeInfoTool="clr-namespace:MichaelBrandonMorris.FileSystemSizeInfoTool"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <fileSystemSizeInfoTool:ViewModel x:Key="ViewModel"/>
        <valueConverters:BooleanToVisibilityHiddenConverter x:Key="BooleanToVisibilityHiddenConverter"/>
        <valueConverters:InverseBooleanConverter x:Key="InverseBooleanConverter"/>
    </Application.Resources>
</Application>
