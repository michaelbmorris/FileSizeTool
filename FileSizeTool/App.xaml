<Application x:Class="FileSizeTool.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:FileSizeTool"
             xmlns:valueConverters="clr-namespace:ValueConverters;assembly=ValueConverters"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
         <local:ViewModel x:Key="ViewModel"/>
        <valueConverters:InverseBooleanConverter x:Key="InverseBooleanConverter" />
    </Application.Resources>
</Application>
